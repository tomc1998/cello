(struct foo
  a u64
  b u64)

(fn get_string_slice __slice_u8 () "Hello")

(fn extern printf void (fmt (ptr u8) &rest))

(fn extern c_do_stuff void ())

(fn add u64 (a u64 b u64)
  (+ a b))

(fn field_a u64 (f foo) (field f a))

(fn field_b u64 (f foo) (field f b))

(fn add_foo u64 (f foo) (add (field_a f) (field_b f)))

(fn cello_do_stuff void () (c_do_stuff))

(fn cello_print_stuff void ()
  (printf c"Hello %d\n" 3))

(fn test_mut void ()
  (mut x u64 3)
  (set x 4)
  (printf c"TEST MUT (EXPECT 4): %d\n" x))

(fn test_if void ()
  (let x u64 3)
  (let y u64 4)
  (let z u64 5)
  (let max u64 (if (> x y)
    (if (> x z) x (if (> z y) z y))
    (if (> y z) y (if (> z x) z x))))
  (printf c"TEST IF (EXPECT 5): Max of %d, %d, and %d: %d\n" x y z max))

(fn test_while void (n u64)
  (mut ii u64 0)
  (mut total u64 0)
  (while (< ii (+ n 1))
    (set total (+ total ii))
    (set ii (+ ii 1)))
  (printf c"TEST WHILE: Sum of numbers from 0 to %d: %d\n" n total))
